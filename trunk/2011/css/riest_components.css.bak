/*box*/
.box-corner, .panel-bottom-corner {
	float: left;
	width: 12px;
	height: 4px;
}

.box-top {
	height: 4px;
	font-size: 0;
}

.box-top-left {
	background: url(../images/box_top_left.gif);
}

.box-top-center {
	float: left;
	height: 3px;
	border-top: 1px solid #E1E1E1;
}

.box-top-right {
	background: url(../images/box_top_right.gif);
}

.box-content, .panel-content {
	border-left: 1px solid #E1E1E1;
	border-right: 1px solid #E1E1E1;
}

.box-bottom, .panel-bottom {
	font-size: 0;
}

.box-bottom-left, .panel-bottom-left  {
	background: url(../images/box_bottom_left.gif);
}

.box-bottom-center, .panel-bottom-center {
	float: left;
	height: 3px;
	border-bottom: 1px solid #E1E1E1;
}

.box-bottom-right, .panel-bottom-right  {
	background: url(../images/box_bottom_right.gif);
}

/* panel */
.panel-top {
	height: 38px;
}

.panel-top-corner {
	float: left;
	width: 12px;
	height: 38px;
}

.panel-top-left {
	background: url(../images/panel_top_left.gif);
}

.panel-top-center {
	float: left;
	height: 38px;
	background: url(../images/panel_top_center.gif) repeat-x;
}

.panel-top-right {
	background: url(../images/panel_top_right.gif);
}

.panel-title {
	padding-top: 10px;
}

.panel-title-icon {
	float: left;
	background-image: url(../images/panel_title_icon.gif);
	background-repeat: no-repeat;
	background-position: left center;
	padding-left: 10px;
	color: #8b8b8b;
	text-align: left;
	font-size: 15px;
	font-weight: bold;
}

.panel-title-more {
	float: right;
}

.panel-title-more a img {
	border: none;
}

